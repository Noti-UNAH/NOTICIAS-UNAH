---
import "../plugins/bootstrap/bootstrap.min.css";
import "../css/panelStyle/style.css";
export interface Props {
	title: string;
}

const { title } = Astro.props;

---

<!DOCTYPE html>
<html lang="es">
	<head>
		<meta charset=""/>
		<meta name="viewport" content="width=device-width" />
